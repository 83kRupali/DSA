public String toLowerCase(String s) {
        StringBuilder ans = new StringBuilder();

        for(int i=0; i<s.length(); i++){
            char ch = s.charAt(i);
            if(Character.isUpperCase(ch)){
                ans.append(Character.toLowerCase(ch));
            }
            else{
                ans.append(ch);
            }
        }

        return ans.toString();
    }
